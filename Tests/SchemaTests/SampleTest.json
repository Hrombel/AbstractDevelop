{
  "InstructionSet": [ "in", "out", "rol", "nand", "xor", "jz", "add" ],
  // список тестов с их именами
  "Tests": [
    {
      "Title": "2 х 2 = 15",
      // последовательности ввода-вывода
      "Input": [ 2, 2 ],
      "Output": [ 15 ]
    },
    {
      "Title": "3 x 5 = 4",
      // допустимо вводить литералы в шестнадцетиричном виде
      "Input": [ 3, 5 ],
      "Output": [ "0x04" ]
    },
    {
      "Title": "Test with wrong params",
      // при прочтении параметры вызовут исключение, данный тест будет пропущен
      "Input": [ "-1", 5 ],
      "Output": [ "0x0404" ]
    },
    {
      "Title": "Test with custom instruction set",
      // бинарная форма представления также допустима, количество аргуметов не ограничено (int.MaxValue)
      "Input": [ 0, 0, 0, 0, 238, 45 ],
      "Output": [ "0001001", "10101010" ],
      // усеченный набор инструкций для более сложных случаев
      "InstructionSet": [ "in", "out", "rol", "nand", "jz" ]
    }
  ]
}
